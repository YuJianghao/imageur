{
  "name": "imageur",
  "version": "0.0.2-beta.0",
  "private": true,
  "packageManager": "pnpm@7.0.0",
  "description": "Image pool management site",
  "keywords": [
    "image pool",
    "image management"
  ],
  "license": "MIT",
  "author": "winwin2011 <yjh2011@live.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YuJianghao/imageur.git"
  },
  "scripts": {
    "test": "vitest",
    "start": "node dist/server/index.cjs",
    "build": "npm run typecheck && concurrently -p [{name}] -n 'server,app' -c 'green.bold,blue.bold' 'npm run build:server' 'npm run build:app' ",
    "dev": "concurrently -p [{name}] -n 'server,app' -c 'green.bold,blue.bold' 'npm run dev:server' 'npm run dev:app' ",
    "build:server": "rollup -c",
    "dev:server": "nodemon",
    "build:app": "vite build",
    "dev:app": "vite --open",
    "typecheck": "tsc --noEmit && vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "standard-version --releaseCommitMessageFormat='chore: release v{{currentTag}}'"
  },
  "bugs": {
    "url": "https://github.com/YuJianghao/imageur/issues"
  },
  "homepage": "https://github.com/YuJianghao/imageur#readme",
  "dependencies": {
    "@winwin/koa-authentication": "^0.2.2",
    "http-errors": "^2.0.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-logger": "^3.2.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.23.1",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/koa-logger": "^3.1.2",
    "@types/koa__router": "^8.0.11",
    "@types/rollup-plugin-auto-external": "^2.0.2",
    "concurrently": "^7.2.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.16.0",
    "esno": "^0.16.3",
    "nodemon": "^2.0.16",
    "rollup": "^2.74.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-node-externals": "^4.0.0",
    "standard-version": "^9.5.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vitest": "^0.12.9",
    "vue-tsc": "^0.34.15"
  }
}
