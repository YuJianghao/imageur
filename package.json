{
  "name": "imageur",
  "version": "0.0.1",
  "private": true,
  "description": "Image pool management site",
  "keywords": [
    "image pool",
    "image management"
  ],
  "license": "MIT",
  "author": "winwin2011 <yjh2011@live.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YuJianghao/imageur.git"
  },
  "scripts": {
    "test": "vitest",
    "start": "node dist/server/index.mjs",
    "build": "npm run typecheck && concurrently -p [{name}] -n 'server,app' -c 'green.bold,blue.bold' 'npm run build:server' 'npm run build:app' ",
    "dev": "concurrently -p [{name}] -n 'server,app' -c 'green.bold,blue.bold' 'npm run dev:server' 'npm run dev:app' ",
    "build:server": "unbuild",
    "dev:server": "unbuild --stub && esno dist/server/index.mjs",
    "build:app": "vite build",
    "dev:app": "vite --open",
    "typecheck": "tsc --noEmit && vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "bugs": {
    "url": "https://github.com/YuJianghao/imageur/issues"
  },
  "homepage": "https://github.com/YuJianghao/imageur#readme",
  "dependencies": {
    "koa": "^2.13.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.23.1",
    "@types/koa": "^2.13.4",
    "concurrently": "^7.2.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.16.0",
    "esno": "^0.16.3",
    "typescript": "^4.6.4",
    "unbuild": "^0.7.4",
    "vite": "^2.9.9",
    "vitest": "^0.12.9",
    "vue-tsc": "^0.34.15"
  }
}
